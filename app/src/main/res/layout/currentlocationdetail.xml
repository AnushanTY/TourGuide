<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.anushan.tourguide.CurrentLocationDetail">

  <com.google.android.gms.maps.MapView
      android:layout_width="match_parent"
      android:layout_height="250dp"
      android:id="@+id/mapviewforCurrentLocation">

  </com.google.android.gms.maps.MapView>

  <LinearLayout
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:orientation="horizontal">
    <TextView

        android:layout_width="100dp"
        android:layout_height="50dp"
        android:text="Longtite is"
        android:textColor="@color/black"
        android:textSize="20dp"
       />
    <TextView

        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="longtite"
        android:textColor="@color/black"
        android:textSize="20dp"
        android:id="@+id/lon"
        />

  </LinearLayout>
  <LinearLayout
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:orientation="horizontal">
  <TextView

      android:layout_width="100dp"
      android:layout_height="50dp"
      android:text="Latitute is"
      android:textColor="@color/black"
      android:textSize="20dp"
     />
    <TextView

        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="latitute"
        android:textColor="@color/black"
        android:textSize="20dp"
        android:id="@+id/lat"
        />
  </LinearLayout>

  <TextView

      android:background="@color/normalcolor"
      android:textColor="@color/white"
      android:layout_marginTop="20dp"
      android:layout_marginLeft="10dp"
      android:layout_marginRight="10dp"
      android:id="@+id/multiAutoCompleteTextView"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:text="MultiAutoCompleteTextView" />
</LinearLayout>
